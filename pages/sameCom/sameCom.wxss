page{
  width: 100%;
  height: 100%;
  background: #F4F4F4
}
/* 相关企业 */
.chan{
  box-sizing: border-box;
  height: 45px;
  width: 100%;
  background: #fff;
  font-size: 32rpx;
  border-bottom: 1px solid #E5E5E5;
  overflow: hidden;
   
}
.chan_word{
  float: left;
  margin-top: 14px;
  box-sizing: border-box;
  padding-left: 29rpx;
  border-left: 3px solid #008EFF;
  font-size: 32rpx;
  color: #000;
}
.chan_right{
  float: right;
  overflow: hidden;
  height: 45px;
}
.checkMore{
  display: inline-block;
  margin-right: 10px;
  font-size: 26rpx;
  color: #008EFF;
  line-height: 45px;
}
.right_jiantou{
  width: 16rpx;
  height: 26rpx;
  margin: 10rpx 30rpx 0 0;
}
.list_con{
  box-sizing: border-box;
  padding: 0 30rpx;
  
}
.list_item{
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  background: #fff;
  margin:20rpx 0;
  border-radius: 5px;
}
.list_header{
  overflow: hidden;
}
.logo_con{
  float: left;
  width: 85%;
  font-size: 34rpx;
}
.list_symbol{
  float: left;
  /* display: inline-block; */
  /* margin-right: 20rpx; */
  width: 40rpx;
  height: 40rpx;
  font-size: 24rpx;  
  line-height: 40rpx;
  text-align: center;
  background: pink; 
  color: #fff;
}
.list_fen{
  float: right;
  font-size: 24rpx;
  color: #757575;
}
.fen{
  font-size: 36rpx;
  color: #F96060;
}
.chan_con{
  font-size: 28rpx;
  color: #888;
  margin-top: 10px;
  overflow: hidden;
}
.chan_title{
  float: left;
  box-sizing: border-box;
  padding: 7rpx 10rpx;
  background: #CCE8FF;
  color: #008EFF;
  margin-right: 20rpx;
  /* width: 144rpx; */
}
.chanItem_con{
  float: left;
  width: 75%; 
}
.chan_item{
  display: inline-block;
  box-sizing: border-box;
  padding: 6rpx 10rpx;
  border-radius:4rpx;
  color: #888888;
  font-size: 26rpx;
  position: relative;
}
.symbol{
  width:6rpx;
  height:6rpx;
  border-radius: 3rpx;
  background:rgba(136,136,136,1);
  position: absolute;
  right: -5rpx;
  top: 22rpx;
}
.com_logo{
  float: left;
  width: 38rpx;
  height: 38rpx;
  margin-top:5rpx;
}
.item_name{
  float: left;
  font-size: 34rpx;
  color: #000;
  width: 485rpx;
  margin-left: 10rpx;
}
.school_name{
  width: 620rpx;
}
.schoolLogo_con{
  width: 100%;
}
.styleOnly{
  border: none
}